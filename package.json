    "apple-signin-auth": "^1.7.4",
    "aws-sdk": "^2.1464.0",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "dotenv": "^10.0.0",
    "extract-zip": "^2.0.1",
    "fastify": "^3.21.6",
    "fastify-cors": "^6.0.3",
    "fs-extra": "^10.0.0",
    "google-auth-library": "^7.11.0",
    "jest": "^27.2.1",
    "jose": "^4.3.7",
    "jwt-decode": "^3.1.2",
    "knex": "^0.95.11",
    "njwt": "^1.1.0",
    "nodemon": "^2.0.15",
    "openpgp": "^5.0.1",
    "path-to-regexp": "^6.2.0",
    "pg": "^8.7.1",
